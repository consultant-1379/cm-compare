--><ns2:EnumDataTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ns1:lifeCycle="CURRENT" ns1:name="LedStatus" ns1:ns="CPP_MODEL" ns1:version="3.12.75">
  <ns1:modelCreationInfo author="mom-xml-transformer-tool" creationDate="2013-08-22 16:54:14.262">
    <ns1:derivedModel derivedFrom="/oss_edt/CPP_MODEL/CPP_MODEL/3.12.75"/>
  </ns1:modelCreationInfo>
  <ns1:desc>Possible states for green, yellow and red LEDs. Since the semantics of the different LED status depend on the color of the LED, the semantics is described as part of the corresponding LED status attribute description.</ns1:desc>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="NOT_APPLICABLE" value="0">
    <ns1:desc>The LED status is not supported.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="NOT_AVAILABLE" value="1">
    <ns1:desc>The LED status is currently not available.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="OUT" value="2">
    <ns1:desc>See the description of the corresponding LED status attribute.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="STEADY_LIGHT" value="3">
    <ns1:desc>See the description of the corresponding LED status attribute.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="SLOW_BLINK" value="4">
    <ns1:desc>See the description of the corresponding LED status attribute.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="FAST_BLINK" value="5">
    <ns1:desc>Load or test in progress.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="FLICKERING" value="6">
    <ns1:desc>Busy. See the description of the corresponding LED status attribute.</ns1:desc>
  </ns2:member>
</ns2:EnumDataTypeDefinition>
