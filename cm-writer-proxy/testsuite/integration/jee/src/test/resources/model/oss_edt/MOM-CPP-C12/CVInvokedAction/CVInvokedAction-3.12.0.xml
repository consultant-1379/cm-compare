  <ns1:desc>Configuration version invoked action</ns1:desc>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="CONFIRM_RESTORE" value="0">
    <ns1:desc>Makes the restore permanent.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="FORCED_RESTORE" value="1">
    <ns1:desc>Restores the node to a previously downloaded backup CV irrespectively of warnings are detected or not during the verification phase.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="GET_FROM_FTP_SERVER" value="2">
    <ns1:desc>Downloads a remote backup of a CV to the node.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="PUT_TO_FTP_SERVER" value="3">
    <ns1:desc>Creates a backup of a CV existing on the node and uploads it to an FTP server.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="RESTORE" value="4">
    <ns1:desc>Restores the node to a previously downloaded backup CV.</ns1:desc>
  </ns2:member>
  <ns2:member ns1:lifeCycle="CURRENT" ns1:name="VERIFY_RESTORE" value="5">
